{
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
          "default_title": "IG Download"
    },
    "content_scripts": [ {
          "js": [ "jquery.min.js", "igdownload.js"  ],
          "matches": [ "http://*.instagram.com/p/*", "https://*.instagram.com/p/*", "http://*.instagram.com/tv/*", "https://*.instagram.com/tv/*" ]
    } ],
    "description": "Display url of media on ig pages",
    "icons": {
          "128": "icon.png"
    },
    "manifest_version": 2,
    "name": "IG Download",
    "version": "1",
    "background": {
        "scripts": ["jquery.min.js"]
    }
}
