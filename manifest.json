{
  "browser_action": {
    "default_icon": "icon.png",
          "default_title": "IG Download"
    },
    "content_scripts": [ {
          "js": [ "igdownload.js"  ],
          "matches": [ "http://*.instagram.com/p/*", "https://*.instagram.com/p/*" ]
    } ],
    "description": "Display url of media on ig pages",
    "icons": {
          "128": "icon.png"
    },
    "manifest_version": 2,
    "name": "IG Download",
    "version": "1"
}
